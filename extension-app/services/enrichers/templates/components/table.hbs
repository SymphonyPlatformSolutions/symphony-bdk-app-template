<table>
  <thead>
    {{#each headers}}
      <td><b><span style="font-size: 12px; padding: 0">{{this}}</span></b></td>
    {{/each}}
  </thead>
  <tbody>
    {{#each rows}}
      <tr>
        {{#each this}}
          <td style="padding: 2px 0 0 0; font-size: 10px;">{{{this}}}</td>
        {{/each}}
      </tr>
    {{else}}
      <tr>
        <td>Notification came with unidentifiable changes</td>
        <td>--</td>
        <td>--</td>
      </tr>
    {{/each}}
  </tbody>
</table>
